<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GameFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/game_top_body"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/quit_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="@string/quit"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip1"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:layout_marginStart="50dp"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip2"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip3"
        app:layout_constraintStart_toEndOf="@id/ourShip1"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip3"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip4"
        app:layout_constraintStart_toEndOf="@id/ourShip2"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip4"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip5"
        app:layout_constraintStart_toEndOf="@id/ourShip3"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip5"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip6"
        app:layout_constraintStart_toEndOf="@id/ourShip4"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip6"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip7"
        app:layout_constraintStart_toEndOf="@id/ourShip5"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip7"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip8"
        app:layout_constraintStart_toEndOf="@id/ourShip6"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip8"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip9"
        app:layout_constraintStart_toEndOf="@id/ourShip7"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip9"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/ourShip10"
        app:layout_constraintStart_toEndOf="@id/ourShip8"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ourShip10"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@id/ourShip9"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview1"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        android:layout_marginStart="130dp"
        app:layout_constraintStart_toEndOf="@id/ourShip10"
        app:layout_constraintEnd_toStartOf="@id/recyclerview2"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview2"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview3"
        app:layout_constraintStart_toEndOf="@id/recyclerview1"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview3"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview4"
        app:layout_constraintStart_toEndOf="@id/recyclerview2"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview4"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview5"
        app:layout_constraintStart_toEndOf="@id/recyclerview3"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview5"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview6"
        app:layout_constraintStart_toEndOf="@id/recyclerview4"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview6"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview7"
        app:layout_constraintStart_toEndOf="@id/recyclerview5"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview7"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview8"
        app:layout_constraintStart_toEndOf="@id/recyclerview6"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview8"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview9"
        app:layout_constraintStart_toEndOf="@id/recyclerview7"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview9"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@id/recyclerview10"
        app:layout_constraintStart_toEndOf="@id/recyclerview8"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview10"
        android:layout_width="@dimen/cell_dimen"
        android:layout_height="@dimen/rec_dimen"
        android:visibility="visible"
        android:layout_marginEnd="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/recyclerview9"
        app:layout_constraintTop_toBottomOf="@id/game_top_body" />

</androidx.constraintlayout.widget.ConstraintLayout>
